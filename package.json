{
  "name": "blinktrade",
  "version": "0.1.0-beta.4",
  "description": "BlinkTrade Client for node.js",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "",
    "lint": "eslint src test",
    "flow": "flow",
    "check": "yarn run lint && yarn run test ",
    "build:src": "babel src --out-dir lib",
    "build:web:commonjs": "webpack",
    "build:web:commonjs:min": "cross-env NODE_ENV=production webpack",
    "build:browser": "webpack --config webpack.config.browser.js",
    "build:browser:min": "cross-env NODE_ENV=production webpack --config webpack.config.browser.js",
    "build:es": "cross-env NODE_ENV=es rollup -c -o es/blinktrade.js",
    "build:umd": "cross-env NODE_ENV=development rollup -c -o dist/blinktrade.js",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c -o dist/blinktrade.min.js",
    "build:commonjs": "cross-env NODE_ENV=cjs rollup -c -o lib/blinktrade.js",
    "build:prod": "yarn run build:commonjs:min && yarn run build:browser:min",
    "build:dev": "yarn run build:commonjs && yarn run build:browser",
    "build": "yarn run build:src && yarn run build:dev && yarn run build:prod",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "blinktrade",
    "node"
  ],
  "jest": {
    "testEnvironment": "jest-environment-node"
  },
  "author": "Cesar Augusto, Rodrigo Souza",
  "license": "GPLv3",
  "dependencies": {
    "create-hmac": "^1.1.7",
    "eventemitter2": "^5.0.1",
    "fetch-ponyfill": "^6.0.2",
    "fingerprintjs2": "1.6.1",
    "ip": "^1.1.3",
    "macaddress": "^0.2.8",
    "nodeify": "^1.0.0",
    "ramda": "^0.25.0",
    "ws": "^5.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "babel-jest": "^22.2.2",
    "babel-loader": "7.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-ramda": "^1.6.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.1",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "flow-bin": "^0.60.1",
    "jest": "^22.2.2",
    "jest-cli": "^22.2.2",
    "jsdom": "^11.6.2",
    "mock-socket": "^7.1.0",
    "rollup": "^0.59.0",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-hypothetical": "^2.1.0",
    "rollup-plugin-ignore": "^1.0.3",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^3.0.0",
    "sinon": "^4.3.0"
  }
}
